{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"test": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test:e2e": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"lint": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"build": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		}
	}
}
